<p id="notice"><%= notice %></th>

<h1> <%= @data_set.title %> </h1>
<table>
  <thead>
    <tr>
      <th> <%= @data_set.id_title %> </th>
      <th> <%= @data_set.x_title %> </th>
      <th> <%= @data_set.y_title %> </th>
      <th> <%= @data_set.radius_title %> </th>
    </tr>
  </thead>

  <tbody>
  <% @data_set.row_items.each do |row| %>
    <tr>
      <td> <%= row.id_text %> </td>
      <td> <%= row.x_data %> </td>
      <td> <%= row.y_data %> </td>
      <td> <%= row.radius_data %> </td>
    </tr>
  </tbody>
  <% end %>
</table>

<%= link_to 'Edit', edit_data_set_path(@data_set) %> |
<%= link_to 'Back', data_sets_path %>
